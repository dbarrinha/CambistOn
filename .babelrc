{
    "presets": [
      "module:metro-react-native-babel-preset"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "babelrc",
          "root": ["./src"],
          "extensions": [".js"],
          "alias": {
            "screens": "./src/screens",
            "components": "./src/components"
          }
        }
      ]
    ]
  }